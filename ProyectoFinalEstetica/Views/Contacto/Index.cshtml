

<h1>Para poder ingresar complete con su cotraseña:</h1>
<hr />

<form asp-action="Index" method="get">
	<div class="row">
		<div class="col-md-4">
			<form asp-action="Index">
				<div asp-validation-summary="ModelOnly" class="text-danger"></div>
				<div class="form-group">
					<label class="control-label">Ingrese la contraseña:</label>
					<input class="form-control" type="password" id="password" name="password" />
					<span class="text-danger"></span>
				</div>
				<div class="form-group">
					<input type="submit" value="Buscar" class="btn btn-primary mt-3" />
				</div>
			</form>
		</div>
	</div>

</form>
<a asp-action="Index" asp-controller="Home" class="btn btn-info" style="margin-top:10px">Volver</a>
